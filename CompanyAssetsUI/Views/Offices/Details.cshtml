@model CompanyAssetsUI.Models.Office

@{
    ViewData["Title"] = "Details";
}

<span class="display-4 text-info">Details</span><span> for office</span>

<div>
    
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.OfficeName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.OfficeName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.OfficeDescription)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.OfficeDescription)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.OfficeActive)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.OfficeActive)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Currency)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Currency.CurrencyName)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Country)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Country.CountryDescription)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.OfficeID" class="btn btn-outline-info btn-sm">Edit</a> 
    <a asp-action="Index" class="btn btn-outline-info btn-sm">Back to List</a>
</div>
